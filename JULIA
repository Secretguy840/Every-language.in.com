abstract type Shape end

struct Circle <: Shape
    radius::Float64
end

struct Rectangle <: Shape
    width::Float64
    height::Float64
end

area(s::Circle) = Ï€ * s.radius^2
area(s::Rectangle) = s.width * s.height

println(area(Circle(1.0)))    # 3.141592653589793
println(area(Rectangle(2,3))) # 6
